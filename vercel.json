{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/simple",
      "dest": "/api/simple.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "true"
  }
}